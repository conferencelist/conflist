        <script src="js/jquery-1.7.1.min.js"></script>
        <script src="js/bootstrap.min.js"></script>
        <script src="js/jquery.backstretch.min.js"></script>
        <script src="js/smoothscrolling.js"></script>
        <script src="js/jquery.prettyPhoto.js"></script>
        <script src="js/jquery.tablesorter.min.js"></script>
        <script>
            $.backstretch("img/background.jpg");
        </script>
        <script>
            <!-- Jquery Lightbox plugin -->
            $(document).ready(function(){
                $("a[rel^='intro']").prettyPhoto({
                    social_tools : false,
                    show_title: false,
                    default_width: 640,
                    default_height:480
                });
            });
            
            <!-- Enable sorting
            var textExtractor = function(node){ 
                timestamp = node.getAttributeNode("data-timestamp");
                if( timestamp != undefined ){  
                    return timestamp.value;
                } else {
                    return $(node).text()
                }        
            }
            $(document).ready(function() 
            {                 
                $("#ongoing").tablesorter( {textExtraction: textExtractor}); 
                $("#upcoming").tablesorter({textExtraction: textExtractor} );
                $("#past").tablesorter({textExtraction: textExtractor} );        
            } 
        );   
            
        </script>
        <script src="http://gitforked.com/api/1.1/button.js" type="text/javascript"></script>

